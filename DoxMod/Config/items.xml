		<!--<triggered_effect trigger="onSelfAttackedOther" action="PlaySound" sound="stunbaton_hit5">
			<requirement name="CVarCompare" cvar="stunBatonCharge" operation="Equals" value="5"/>
		</triggered_effect>-->
